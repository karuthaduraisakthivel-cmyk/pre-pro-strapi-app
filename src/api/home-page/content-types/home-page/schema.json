    "HeroHeader": {
      "type": "component",
      "repeatable": true,
      "component": "custom.header"
    },
    "Achievements": {
      "type": "component",
      "repeatable": false,
      "component": "text.title-paragraph"
    },
    "AchievementMetrics": {
      "type": "component",
      "repeatable": true,
      "component": "text.title-subtitle"
    },
    "OfferingsTitle": {
      "type": "string"
    },
    "Offerings": {
      "type": "component",
      "repeatable": true,
      "component": "custom.offering"
    },
    "Analytics": {
      "type": "component",
      "repeatable": false,
      "component": "card.item"
    },
    "OurClientsTitle": {
      "type": "string"
    },
    "OurClientsParagraph": {
      "type": "text"
    },
    "OurClientDetail": {
      "type": "component",
      "repeatable": true,
      "component": "custom.rotating-card"
    },
    "OurClientImagesTitle": {
      "type": "string"
    },
    "OurClientImages": {
      "type": "component",
      "repeatable": true,
      "component": "custom.header"
    },
    "BlogsTitle": {
      "type": "string"
    },
    "Blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    },
    "NewsTitle": {
      "type": "string"
    },
    "News": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::news.news"
    },
    "SuccessStoryTitle": {
      "type": "string"
    },
    "SuccessStory": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::success-story.success-story"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "BackgroundImages": {
      "type": "component",
      "repeatable": true,
      "component": "media.image"
    }
  }
